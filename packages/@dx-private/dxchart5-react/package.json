{
  "name": "@dx-private/dxchart5-react",
  "version": "5.4.0",
  "author": "devexperts",
  "license": "UNLICENSED",
  "scripts": {
    "//<----dev server---->": "dev server",
    "watch": "yarn ts-node --project ./tsconfig.node.json ./scripts/build-tools/scripts/build-lib.ts ./src ./dist -p ./tsconfig.json -f -w",
    "//<----dev builds---->": "dev builds",
    "build": "yarn clean_dist && yarn build:lib && yarn build:standalone",
    "build:lib": "yarn ts-node --project ./tsconfig.node.json ./scripts/build-tools/scripts/build-lib.ts ./src ./dist -p ./tsconfig.json -f",
    "build:standalone": "webpack --config=webpack-bundle.config.js --env entryPoint=./src/standalone/index-standalone.tsx --env bundlePath=dist --env bundleName=chart-react",
    "//<----prod builds---->": "production builds (no source maps)",
    "build:production": "yarn clean_dist && yarn build:lib:production && yarn build:standalone:production",
    "build:lib:production": "yarn ts-node --project ./tsconfig.node.json ./scripts/build-tools/scripts/build-lib.ts ./src ./dist -p ./tsconfig.json -f -prod && yarn obfuscate:lib && yarn add_disclaimer",
    "build:standalone:production": "webpack --config=webpack-bundle.config.js --env MODE=production --env entryPoint=./src/standalone/index-standalone.tsx --env bundlePath=dist --env bundleName=chart-react",
    "build:trial-standalone-w:production": "yarn add_trialwatermark && webpack --config=webpack-bundle.config.js --env MODE=production --env entryPoint=./src/standalone/index-standalone.tsx --env bundlePath=dist --env bundleName=chart-react && yarn add_trialwatermark --restore && yarn obfuscate:lib ${0} && yarn add_disclaimer",
    "build:trial-lib-w:production": "yarn clean_dist && yarn add_trialwatermark && yarn ts-node --project ./tsconfig.node.json ./scripts/build-tools/scripts/build-lib.ts ./src ./dist -p ./tsconfig.json -f -prod --trial && yarn add_trialwatermark --restore && yarn obfuscate:lib ${0} && yarn add_disclaimer",
    "build:trial-standalone:production": "webpack --config=webpack-bundle.config.js --env MODE=production --env entryPoint=./src/standalone/index-standalone.tsx --env bundlePath=dist --env bundleName=chart-react && yarn obfuscate:lib ${0} && yarn add_disclaimer",
    "build:trial-lib:production": "yarn clean_dist && yarn ts-node --project ./tsconfig.node.json ./scripts/build-tools/scripts/build-lib.ts ./src ./dist -p ./tsconfig.json -f -prod --trial && yarn obfuscate:lib ${0} && yarn add_disclaimer",
    "//<----dxcity builds---->": "build will run on dxcity CI when trying to merge PR builds",
    "build:shadow_merge": "yarn test && yarn build:lib && yarn build:standalone",
    "//<----post process---->": "post-processign scripts",
    "add_trialwatermark": "yarn ts-node --project ./tsconfig.node.json ./scripts/build-tools/scripts/trial-watermark/add-trial-watermark.ts",
    "obfuscate:lib": "yarn ts-node --project ./tsconfig.node.json ../scripts/obfuscate-lib.ts",
    "add_disclaimer": "yarn ts-node --project ./tsconfig.node.json ../scripts/add-disclaimer.ts",
    "test:lint": "eslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "test:lint:fix": "prettier --write ./**/*.ts ./**/*.tsx && eslint --fix \"./**/*.ts\" \"./**/*.tsx\"",
    "test:unit": "jest",
    "test": "yarn test:lint && yarn test:unit",
    "clean": "yarn clean_dist && rimraf ./generated",
    "clean_dist": "rimraf ./dist"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "ace-builds": "^1.4.13",
    "color": "^3.1.2",
    "comlink": "4.4.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^1.3.8",
    "fp-ts": "^2.3.1",
    "fp-ts-rxjs": "^0.6.15",
    "monocle-ts": "^2.3.13",
    "re-resizable": "^6.9.0",
    "react-ace": "^9.3.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-colorful": "^5.5.1",
    "react-draggable": "^4.4.5",
    "react-spring": "^9.3.0",
    "react-use-measure": "^2.0.4",
    "rxjs": "^7.5.7",
    "scroll-into-view-if-needed": "^2.2.20",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@react-spring/types": "^9.2.2",
    "@types/accounting": "^0.4.1",
    "@types/color": "^3.0.1",
    "@types/jest": "^24.0.19",
    "@types/node": "^16.3.2",
    "@types/prettier": "^2.3.2",
    "@types/react": "18.0.27",
    "@types/react-beautiful-dnd": "^13.1.3",
    "@types/react-dom": "18.0.10",
    "@types/react-resizable": "^1.7.2",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/strong-log-transformer": "^1.0.0",
    "@types/styled-components": "^5.1.2",
    "chalk": "^4.1.2",
    "commander": "^7.2.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.4.0",
    "esbuild-jest": "^0.5.0",
    "esbuild-loader": "^3.0.1",
    "eslint": "^8.41.0",
    "execa": "^5.1.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-react-parser": "^0.14.0",
    "http-proxy-middleware": "^1.0.6",
    "javascript-obfuscator": "^4.0.2",
    "jest": "^24.9.0",
    "prettier": "^2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "^3.0.0",
    "strong-log-transformer": "^2.1.0",
    "style-loader": "^3.3.1",
    "sync-glob": "^1.4.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "typescript-plugin-styled-components": "^2.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.1.1",
    "webpack-obfuscator": "^3.5.1"
  },
  "peerDependencies": {
    "@types/react": ">=17.0.2",
    "@types/react-dom": ">=17.0.2",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://nexus.in.devexperts.com/repository/npm-dx-private/"
  }
}
